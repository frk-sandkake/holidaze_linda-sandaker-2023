{
  "name": "holidaze-ts_frk-sandkake",
  "description": "Online booking website for lodging",
  "keywords": ["vitejs", "react", "typescript", "redux", "axios", "online booking", "rest api"],
  "author": "Linda Sandaker",
  "repository": "https://github.com/frk-sandkake/holidaze_linda-sandaker-2023",
  "homepage": "https://holi-daze.netlify.app/",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "pre-commit": "lint-staged",
    "prepare": "husky install"
  },
  "lint-staged": {
